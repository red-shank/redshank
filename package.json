{
  "name": "redshank",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "examples/**/*",
    "docs/*"
  ],
  "scripts": {
    "co": "sui-mono commit",
    "release": "sui-mono release",
    "check": "sui-mono check",
    "doc:start": "yarn workspace website run dev",
    "example:start": "yarn workspace native_examples_basic run start",
    "watch:native": "yarn workspace @redshank/native run build:watch",
    "deploy:native": "yarn workspace @redshank/native run deploy",
    "postinstall": "node scripts/postinstall/index.js"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@s-ui/mono": "2.45.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/react-test-renderer": "^18.3.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-native": "^12.5.1",
    "@types/react": "~18.2.45",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.2.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.3.2",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.4.5"
  },
  "overrides": {
    "react-test-renderer": {
      "react": "^18.2.0"
    }
  }
}
